pipeline {
    agent { label 'jenkins-nodejs-8' }

    environment {
        USER_CREDENTIALS = credentials('nicksutest')
    }

    stages {
        stage('Run') {
            steps {
                sh "echo $USER_CREDENTIALS_USR"
                sh "echo $USER_CREDENTIALS_PSW"
                sh "curl -u $USER_CREDENTIALS_USR:$USER_CREDENTIALS_PSW https://issuetracking.maaii.com:9443/rest/api/content?spaceKey=~nicksu"
            }
        }
    }
}